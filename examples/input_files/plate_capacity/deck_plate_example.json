{
  "_template_info": {
    "name": "Deck Plate Analysis Example",
    "description": "Example plate buckling analysis for offshore platform deck structure",
    "analysis_type": "plate_buckling"
  },

  "basename": "plate_buckling",
  "calculation_type": "DNV_rp_C201",

  "default": {
    "units": {
      "length": "m",
      "stress": "Pa",
      "force": "N"
    },
    "log_level": "INFO",
    "config": {
      "overwrite": {
        "output": true
      }
    }
  },

  "environment": {
    "water_depth": 100.0,
    "seawater_density": 1025.0,
    "temperature": {
      "ambient": 15.0,
      "operating": 40.0
    }
  },

  "groups": [
    {
      "plate_id": "DECK_P001",
      "description": "Main deck plate panel between primary beams",
      "plate_type": "stiffened_panel",

      "geometry": {
        "length": 4.0,
        "breadth": 2.5,
        "thickness": 0.020,
        "stiffener_spacing": 0.6
      },

      "material": {
        "grade": "S355",
        "yield_strength": 355000000,
        "young_modulus": 210000000000,
        "poisson_ratio": 0.3,
        "density": 7850
      },

      "loading": {
        "longitudinal_stress": 180000000,
        "transverse_stress": 90000000,
        "shear_stress": 60000000,
        "applied_pressure": 5000,
        "load_case": "ULS_operating"
      },

      "dnv_parameters": {
        "buckling_coefficient": 4.0,
        "plate_slenderness_limit": 30.0,
        "effective_width_factor": 0.8
      },

      "analysis_options": {
        "include_initial_imperfections": true,
        "include_residual_stress": true,
        "nonlinear_analysis": false
      }
    }
  ],

  "load_cases": {
    "ULS_operating": {
      "description": "Ultimate Limit State - Normal Operating with Deck Loads",
      "safety_factor": 1.25,
      "load_factors": {
        "permanent": 1.35,
        "variable": 1.5,
        "environmental": 1.3
      }
    }
  },

  "design_criteria": {
    "DNV_RP_C201": {
      "plate_buckling": {
        "usage_factor_limit": 1.0,
        "serviceability_limit": 0.8
      }
    }
  }
}