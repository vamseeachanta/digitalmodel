  "analysis_methods": {
    "stress_theories": {
      "von_mises": {
        "description": "Von Mises equivalent stress theory",
        "formula": "sqrt(0.5*((σ1-σ2)² + (σ2-σ3)² + (σ3-σ1)²))",
        "applicable_materials": ["ductile"],
        "safety_factor": 1.5
      },
      "tresca": {
        "description": "Maximum shear stress theory",
        "formula": "max(|σ1-σ2|, |σ2-σ3|, |σ3-σ1|)/2",
        "applicable_materials": ["ductile"],
        "safety_factor": 1.5
      },
      "principal_stress": {
        "description": "Maximum principal stress theory",
        "formula": "max(σ1, σ2, σ3)",
        "applicable_materials": ["brittle"],
        "safety_factor": 2.0
      },
      "mohr_coulomb": {
        "description": "Mohr-Coulomb failure criterion",
        "applicable_materials": ["soil", "rock", "concrete"],
        "parameters": {
          "friction_angle": 30,
          "cohesion": 50000
        }
      }
    },

    "stress_calculations": {
      "beam_theory": {
        "normal_stress": "M*y/I + P/A",
        "shear_stress": "V*Q/(I*t)",
        "torsional_stress": "T*r/J"
      },
      "pressure_vessel": {
        "hoop_stress": "p*r/t",
        "longitudinal_stress": "p*r/(2*t)",
        "radial_stress": "-p (inner surface)"
      },
      "thermal_stress": {
        "free_expansion": "α*ΔT*E",
        "constrained": "α*ΔT*E/(1-ν)"
      }
    }
  },

