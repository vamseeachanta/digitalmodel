# OrcaFlex General Analysis Settings
# Generated by OrcaFlex Agent
# Project: {{ project_name }}
# Date: {{ generation_date }}

General:
  StageDuration:
    - {{ stage_duration_buildup }}
    - {{ stage_duration_simulation }}
  TargetLogSampleInterval: {{ log_sample_interval }}
  ImplicitConstantTimeStep: {{ time_step }}
  ImplicitVariableMaxTimeStep: ~

StaticsMinDamping: 5
DynamicsSolutionMethod: Implicit time domain
UseCalculatedMeanPositionInStatics: No
UseStaticCablesInStatics: No
FiniteLengthWingDiscretisationTypeBending: ~
FiniteLengthWingDiscretisationTypeTorsion: ~

# Line contact settings
LineContact: No
LineContactModel: Line centre-to-centre
LineContactClash: Yes
LineContactCrossing: Yes
LineContactMaxIter: 20
LineContactRelTol: 0.01
LineContactCheckExtents: Yes
LineContactAbsZTol: 0.1

# Environment
WaveType: {{ wave_type }}
WaveDirection: {{ wave_direction }}
WaveTrains: 1

# Calculation settings
Include6DOFStructureInertia: Yes
StackedBuoyCalculation: No
UseExtendedContactModel: No
